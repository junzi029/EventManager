<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.eventmanage.domain">
 <class name="Event" table="event">
 <id name="eventId" column="event_id">
 <generator class="native" />
 </id>
 <property name="eventTitle" column="event_title" type="string" />
 <property name="startDate" column="start_date" type="timestamp" />
 <property name="endDate" column="end_date" type="timestamp" />
 <property name="targetGroup" column="target_group" type="integer" />
 <property name="detail" column="detail" type="string" />
 <property name="userId" column="user_id" type="integer" />
 <property name="createdAt" column="created_at" type="timestamp" />

 </class>
</hibernate-mapping>