<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.eventmanage.domain">
 <class name="UserType" table="user_type">
 <id name="attendId" column="attend_id">
 <generator class="native" />
 </id>
 <property name="userId" column="user_id" type="integer" />
 <property name="eventId" column="event_id" type="integer" />

 </class>
</hibernate-mapping>