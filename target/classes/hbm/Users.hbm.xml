<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.eventmanage.domain">
	<class name="Users" table="users">
		<id name="userId" column="user_id">
			<generator class="native" />
		</id>
		<property name="loginId" column="login_id" type="string" />
		<property name="loginPass" column="login_pass" type="string" />
		<property name="userName" column="user_name" type="string" />
		<property name="userType" column="user_type" type="integer" />
		<property name="groupId" column="group_id" type="integer" />
		<property name="createdAt" column="created_at" type="timestamp" />
	</class>
</hibernate-mapping>