<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.eventmanage.domain">
	<class name="Attend" table="attend">
		<id name="attendId" column="attend_id">
			<generator class="native" />
		</id>
		<property name="userId" column="user_id" type="int" />
		<property name="eventId" column="event_id" type="int" />
	</class>
</hibernate-mapping>